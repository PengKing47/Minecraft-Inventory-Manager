{
	"schemaVersion": 1,
	"id": "inventorymanager",
	"version": "${version}",
	"name": "InventoryManager",
	"description": "This is aa mod to cycle through the rows of your inventory",
	"authors": [
		"PengKing"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/inventorymanager/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.inventorymanager.InventoryManager"
		],
		"client": [
			"com.inventorymanager.InventoryManagerClientInitializer"
		],
		"fabric-datagen": [
			"com.inventorymanager.InventoryManagerDataGenerator"
		]
	},
	"mixins": [
		"inventorymanager.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-key-binding-api-v1": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}